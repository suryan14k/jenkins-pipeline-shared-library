#%RAML 1.0 DataType
type: object
properties:
  name: string
  description:
    type: string
    required: false
  format:
    type: string
    enum: ['json', 'yaml']
    required: false
  classifier:
    required: false
    enum: ['raml', 'raml-fragment', 'oas']
  type:
    description: Project type
    type: string
    enum: ['raml', 'raml-fragment']
example: !include ../examples/projectCreate.json